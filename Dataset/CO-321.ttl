@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix co_321: <http://www.cropontology.org/co_321#> .

co_321:Variable a owl:Class.

co_321:Method a owl:Class.

co_321:Scale a owl:Class.

co_321:OrdinalScale a owl:Class;
	rdfs:subClassOf co_321:Scale.

##### Définition des traits
co_321:Trait a owl:Class.

co_321:Agronomical a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:BioticSTress a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:AbioticStress a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:Phenological a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:Morphological a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:Quality a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:Physiological a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:OtherTrait a owl:Class;
	rdfs:subClassOf co_321:Trait.

co_321:Environmental a owl:Class;
	rdfs:subClassOf co_321:Trait.
#####
co_321:Category a owl:Class.

co_321:hasInstitution a owl:DatatypeProperty;
	rdfs:label "a pour institut"@fr;
	rdfs:comment "Institut d'appartenance"@fr;

co_321:hasVariableXRef a owl:DatatypeProperty;
	rdfs:label "a pour référence"@fr;
	rdfs:comment "Référence de la variable"@fr.

co_321:hasMethodClass a owl:ObjectProperty;
	rdfs:label ""@fr;
	rdfs:comment ""@fr.
	rdfs:domain co_321:Variable;
	rdfs:range co_321:Method.

co_321:hasTrait a owl:ObjectProperty;
	rdfs:label ""@fr;
	rdfs:comment ""@fr;
	rdfs:domain co_321:Variable;
	rdfs:range co_321:Trait.

co_321:hasScale a owl:ObjectProperty;
	rdfs:label ""@fr;
	rdfs:comment ""@fr;
	rdfs:domain co_321:Variable;
	rdfs:range co_321:Scale.

co_321:hasId a owl:DatatypeProperty;
	rdfs:label ""@fr;
	rdfs:comment ""@fr;